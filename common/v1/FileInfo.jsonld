{
  "@context": {
    "spdci": "https://schema.spdci.org/common/v1/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@id": "spdci:FileInfo",
  "@type": "rdfs:Class",
  "rdfs:comment": "File info. Used in file upload feature using HTTPS",
  "rdfs:label": "File Information",
  "rdfs:subClassOf": [
    {
      "@type": "rdfs:Class",
      "rdfs:label": "Structured Entity"
    }
  ],
  "rdfs:property": [
    {
      "@id": "spdci:action",
      "@type": "rdfs:Datatype",
      "rdfs:domain": "spdci:FileInfo",
      "rdfs:comment": "G2P Connect specific actions. Usually verb from the URI should go here to help store and fwd kind of processing requirements.",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "spdci:fileName",
      "@type": "rdfs:Datatype",
      "rdfs:domain": "spdci:FileInfo",
      "rdfs:comment": "Disbursement instruction file representing Disburse or OnDisburse end point elements i.e signature/header/message entities as a file record",
      "rdfs:range": "xsd:string"
    },
    {
      "@id": "spdci:fileFormat",
      "@type": "rdfs:Datatype",
      "rdfs:domain": "spdci:FileInfo",
      "rdfs:comment": "File content format. e.g json, csv, etc.",
      "rdfs:range": "xsd:string"
    }
  ]
}

