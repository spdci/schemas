{
  "@context": {
    "@vocab": "http://spdci.org/schemas/common/v1/",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "schema": "http://schema.org/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#"
  },
  "@graph": [
    {
      "@id": "spdci:Relationship",
      "@type": "rdfs:Class",
      "rdfs:comment": "Represents a relationship between a person and another person.",
      "rdfs:property": [
        {
          "@id": "spdci:person",
          "@type": "rdfs:Datatype",
          "rdfs:domain": "spdci:Relationship",
          "rdfs:range": [
            "@id",
            "spdci:CRVS_Person"
          ],
          "rdfs:comment": "Identifier of the person related to the applicant."
        },
        {
          "@id": "spdci:relationship_type",
          "@type": "rdfs:ObjectProperty",
          "rdfs:domain": "spdci:Relationship",
          "rdfs:range": "spdci:RelationshipTypeEnum",
          "rdfs:comment": "Type of relationship between the applicant and the related person."
        }
      ]
    },
    {
      "@id": "spdci:RelationshipTypeEnum",
      "@type": "rdfs:Class",
      "rdfs:comment": "Enumeration for relationship types.",
      "rdfs:subClassOf": "rdfs:Resource",
      "owl:oneOf": [
        "spdci:Spouse",
        "spdci:Son",
        "spdci:Daughter",
        "spdci:SonInLaw",
        "spdci:DaughterInLaw",
        "spdci:Mother",
        "spdci:Father",
        "spdci:Grandson",
        "spdci:Granddaughter",
