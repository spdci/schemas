        "spdci:LegalGuardian",
        "spdci:HeadOfGroomFamily",
        "spdci:HeadOfBrideFamily",
        "spdci:HeadOfFamily"
      ]
    },
    {
      "@id": "spdci:Spouse",
      "@type": "spdci:RelationshipTypeEnum",
      "rdfs:label": "Spouse",
      "rdfs:comment": "Spouse."
    },
