{
  "@context": {
    "spdci": "http://spdci.org/schemas/src/extensions/cdpi/v1/",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "xsd": "http://www.w3.org/2001/XMLSchema#"
  },
  "@id": "spdci:FinancialAddress",
  "@type": "rdfs:Datatype",
  "rdfs:comment": "Financial address is a case-insensitive normative representation of a store of value account as id-type:id@provider.",
  "rdfs:label": "Financial Address",
  "rdfs:range": "xsd:string",
  "rdfs:pattern": "^[a-zA-Z0-9.-]+@[a-zA-Z0-9.-]+$",
  "rdfs:examples": [
    "token@id-provider",
    "uid@pymt-rail",
    "vid@id-provider",
    "mobile@mobile-provider",
    "account-id@bank-psp-code",
    "account-no@ifsc-code.ifsc.pymt-rail",
    "user-id@psp-code",
    "token@psp-code",
    "code@purpose-code.voucher-provider"
  ]
}
