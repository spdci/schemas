{
    "@context": {
      "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
      "@vocab": "https://schema.spdci.org/extensions/dr/v1/"
    },
    "@graph": [
      {
        "@id": "level:DisabilityLevelEnum",
        "@type": "rdfs:Class",
        "rdfs:subClassOf": "rdfs:Resource",
        "rdfs:label": "Disability Level  Enum",
        "rdfs:comment": "Enumeration for disability levels.",
        "owl:oneOf": [
          "level:1",
          "level:2",
          "level:3",
          "level:4"
        ]
      },
      {
        "@id": "level:1",
        "@type": "category:DisabilityLevelEnum",
        "rdfs:label": "High Support Needs (sometimes called very severe or profound)",
        "rdfs:comment": "Person requires continuous, intensive support for daily living activities. "
      },
      {
        "@id": "level:2",
        "@type": "category:DisabilityLevelEnum",
        "rdfs:label": "Severe",
        "rdfs:comment": "Person experiences significant limitation that limit independence, often needing extensive support."
      },
      {
        "@id": "level:3",
        "@type": "category:FunctionalSeverityEnum",
        "rdfs:label": "Moderate",
        "rdfs:comment": "Person experiences noticeable limitations that may require some accommodations or support occasionally or only for certain tasks."
      },
      {
        "@id": "level:4",
        "@type": "category:DisabilityLevelEnum",
        "rdfs:label": "Mild",
        "rdfs:comment": "Person experiences minor limitations in performing tasks, generally manageable without special support."
      }
    ]
  }
  