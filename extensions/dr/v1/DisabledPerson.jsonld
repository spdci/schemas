{
    "@context": {
      "spdci": "https://schema.spdci.org/extensions/dr/v1/",
      "social": "https://schema.spdci.org/extensions/social/v1/",
      "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
      "xsd": "http://www.w3.org/2001/XMLSchema#",
      "owl": "http://www.w3.org/2002/07/owl#",
      "common": "https://schema.spdci.org/common/v1/"
    },
    "@id": "spdci:DR_Person",
    "@type": "rdfs:Class",
    "rdfs:comment": "This data object contains the information on people  held in the disabled registry",
    "rdfs:label": "DR_Person",
    "rdfs:subClassOf": [
      {
        "@type": "rdfs:Class",
        "rdfs:label": "Structured Entity"
      }
    ],
    "rdfs:property": [
        {
            "@id": "spdci:identifier",
            "@type": "rdfs:Datatype",
            "rdfs:domain": "spdci:DR_Person",
            "rdfs:range": "xsd:string",
            "rdfs:comment": "The identifier for a group that individuals are linked to."
        },
      {
        "@id": "spdci:member",
        "@type": "rdfs:Datatype",
        "rdfs:domain": "spdci:DR_Person",
        "rdfs:range": "social:Member",
        "rdfs:comment": "An individual belonging to a family or household"
      },
      {
        "@id": "spdci:disability_support",
        "@type": "rdfs:Datatype",
        "rdfs:domain": "spdci:DR_Person",
        "rdfs:range": "spdci:DisabilitySupport",
        "rdfs:comment": "The support needed/received by the disabled person."
      },
      {
        "@id": "spdci:human_assistance",
        "@type": "rdfs:Datatype",
        "rdfs:domain": "spdci:DR_Person",
        "rdfs:range": "spdci:HumanAssistanceEnum",
        "rdfs:comment": "Is the level of human assistance the disabled person needs"
      },
      {
        "@id": "spdci:care_giver",
        "@type": "rdfs:Datatype",
        "rdfs:domain": "spdci:DR_Person",
        "rdfs:range": "social:person",
        "rdfs:comment": "An individual belonging to a family or household that is taking care of the registered disabled"
      },
      {
        "@id": "spdci:relationship_type",
        "@type": "rdfs:Datatype",
        "rdfs:domain": "spdci:DR_Person",
        "rdfs:range": "social:RelationshipTypeEnum",
        "rdfs:comment": "The type of relationship disable has to the care giver."
      },
      {
        "@id": "spdci:registration_date",
        "@type": "rdfs:Datatype",
        "rdfs:domain": "spdci:DR_Person",
        "rdfs:range": "common:DateTime",
        "rdfs:comment": "The date of registration of the applicant, formatted according to schema:DateTime."
      },
      {
        "@id": "spdci:last_updated",
        "@type": "rdfs:Datatype",
        "rdfs:domain": "spdci:DR_Person",
        "rdfs:range": "common:DateTime",
        "rdfs:comment": "The date when person details got updated , formatted according to schema:DateTime."
      }
      
    ]
  }
  